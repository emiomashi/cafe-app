<h1>New Cafe</h1>

<%= form_with model: @cafe do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :body %><br>
    <%= form.text_area :body %>
  </div>

  <div>
    <%= form.label :category %><br>
    <%= form.text_area :category %>
  </div>

    <div>
    <%= form.label :rating %><br>
    <%= form.text_area :rating %>
  </div>


  <div>
    <%= form.submit %>
  </div>
<% end %>
